{
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false,
          "targets": {
            "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
          }
        }
      ]
    ],
    "plugins": [
      // "transform-vue-jsx",
      [
        "@babel/plugin-transform-runtime",
        {
          "builtins": true,
          "corejs": { "version": 2 } // 指定 runtime-corejs 的版本，目前有 2 3 两个版本
          // "polyfill": false
        }
      ],
      [
        "import",
        {
          "libraryName": "vant",
          "libraryDirectory": "es",
          "style": true
        }
      ]
    ]
  }